    "animejs": "^3.2.1",
    "mitt": "^3.0.0",
    "typewriter-effect": "^2.19.0",
    "vue": "^3.2.40",
    "vue-next-breakpoints": "^1.0.1",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-syntax-import-assertions": "^7.18.6",
    "@vitejs/plugin-vue": "^3.1.0",
    "eslint": "^8.23.0",
